# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def main_pods
  use_frameworks!

  pod 'Firebase/Core'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Firestore'
  pod 'Firebase/Invites'
  pod 'Firebase/Messaging'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'NSObject+Rx'
  pod 'SVProgressHUD'
  pod 'Alamofire', '~> 4.6'
  pod 'AlamofireImage', '~> 3.1'
  pod 'Stripe'
  pod 'AppRating'
end

target 'TediousCustomer' do
    main_pods
end

target 'TediousProvider' do
    main_pods
end
